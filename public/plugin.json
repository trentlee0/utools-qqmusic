{
  "pluginName": "QQ音乐助手",
  "author": "Trent0",
  "homepage": "https://github.com/trentlee0/utools-qqmusic",
  "description": "QQ音乐的一些快捷操作",
  "version": "1.0.0",
  "logo": "logo.png",
  "preload": "preload.js",
  "platform": [
    "darwin"
  ],
  "features": [
    {
      "code": "qq-play-pause",
      "explain": "播放或暂停",
      "cmds": [
        "Play Pause",
        "播放或暂停"
      ]
    },
    {
      "code": "qq-prev",
      "explain": "上一首",
      "cmds": [
        "Previous Track",
        "上一首"
      ]
    },
    {
      "code": "qq-next",
      "explain": "下一首",
      "cmds": [
        "Next Track",
        "下一首"
      ]
    },
    {
      "code": "qq-like",
      "explain": "喜欢歌曲",
      "cmds": [
        "Like Track",
        "喜欢歌曲"
      ]
    },
    {
      "code": "qq-dislike",
      "explain": "取消喜欢",
      "cmds": [
        "Dislike Track",
        "取消喜欢"
      ]
    },
    {
      "code": "qq-up",
      "explain": "音量加",
      "cmds": [
        "Volume Up",
        "音量加"
      ]
    },
    {
      "code": "qq-down",
      "explain": "音量减",
      "cmds": [
        "Volume Down",
        "音量减"
      ]
    },
    {
      "code": "qq-shuffle",
      "explain": "开关随机播放",
      "cmds": [
        "Toggle Shuffle",
        "开关随机播放"
      ]
    },
    {
      "code": "qq-lyric",
      "explain": "开关桌面歌词",
      "cmds": [
        "Toggle Lyric",
        "开关桌面歌词"
      ]
    },
    {
      "code": "qq-quit",
      "explain": "退出QQ音乐",
      "cmds": [
        "Quit QQMusic",
        "退出QQ音乐"
      ]
    },
    {
      "code": "qq-list",
      "explain": "QQ音乐所有快捷操作",
      "cmds": [
        "QQMusic All Commands",
        "QQ音乐所有命令",
        "QQ音乐助手",
        {
          "type": "window",
          "label": "QQ音乐所有快捷操作",
          "match": {
            "app": [
              "QQMusic.app"
            ]
          }
        }
      ]
    },
    {
      "code": "qq-search",
      "explain": "在QQ音乐中搜索音乐",
      "cmds": [
        "Search Music",
        "搜索音乐",
        "搜索歌曲"
      ]
    }
  ]
}